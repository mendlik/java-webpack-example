  "scripts": {
    "watch": "webpack-dev-server --watch --inline --hot --port 9090 --config ./webpack.babel.js --env.mode watch",
    "start": "webpack-dev-server --watch --config ./webpack.babel.js",
    "build": "webpack --progress --config ./webpack.babel.js",
    "build-debug": "npm run build -- --env.mode debug",
    "build-webjar": "webpack --config ./webpack.babel.js --env.mode webjar",
    "ci": "npm run lint && npm run test && npm run build",
    "test": "mocha --compilers css:./test/cssCompiler.js,js:babel-core/register --require ./test/setup.js './src/**/*.spec.js'",
    "lint": "eslint --max-warnings 0 --cache .",
    "favicons": "babel-node ./lib/favicons",
    "clean": "rimraf ./build "
  },
